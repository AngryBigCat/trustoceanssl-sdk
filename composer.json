{
    "name": "angry-one-cat/trustoceanssl-sdk",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "AngryCat",
            "email": "angrycat123@163.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-openssl": "*",
        "ext-mbstring": "*",
        "acmephp/ssl": "dev-master",
        "guzzlehttp/guzzle": "7.4.x-dev",
        "respect/validation": "9999999-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "TrustOceanSSL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TrustOceanSSL\\Tests\\": "tests/TrustOceanSSL/Tests/"
        }
    },
    "minimum-stability": "dev"
}
